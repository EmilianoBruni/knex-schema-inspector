    "schema",
    "mysql",
    "postgresql",
    "sqlite3",
    "javascript"
  ],
  "author": "Rijk van Zanten <rijkvanzanten@me.com> (https://github.com/rijkvanzanten)",
  "contributors": [
    "Tanya Byrne (https://github.com/kukulaka)",
    "Martti Laine (https://github.com/codeclown)",
    "Martin Benndorf (https://github.com/MiniDigger)",
    "Igor Savin (https://github.com/kibertoad)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/knex/knex-schema-inspector/issues"
  },
  "homepage": "https://github.com/knex/knex-schema-inspector#readme",
  "devDependencies": {
    "@types/chai": "^5.2.2",
    "@types/lodash": "^4.17.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.3.0",
    "better-sqlite3": "^12.2.0",
    "chai": "^5.3.1",
    "husky": "^9.1.7",
    "knex": "^3.1.0",
    "lint-staged": "^16.1.5",
    "mocha": "^11.7.1",
    "mysql": "^2.18.1",
    "oracledb": "^6.9.0",
    "pg": "^8.16.3",
    "prettier": "^3.6.2",
    "sqlite3": "^5.1.7",
    "tedious": "^18.6.1",
    "ts-mocha": "^11.1.0",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "*.{ts, js,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "dependencies": {
    "lodash": "^4.17.21"
